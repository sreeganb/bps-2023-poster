\ProvidesPackageRCS $Header: /cvsroot/latex-beamer/latex-beamer/themes/color/beamercolorthemeUAgrey.sty,v 1.2 2007/01/28 20:48:24 tantau Exp $


\mode<presentation>

\definecolor{UAblue}{RGB}{0,51,102}
\definecolor{UAred}{RGB}{204,0,51}
\definecolor{UAgrey}{RGB}{206,210,210}

\setbeamercolor{section in toc}{fg=UAblue,bg=UAblue}
\setbeamercolor{alerted text}{fg=UAred}
\setbeamercolor*{palette primary}{fg=UAblue,bg=UAgrey}
\setbeamercolor*{palette secondary}{fg=UAblue,bg=UAgrey}
\setbeamercolor*{palette tertiary}{fg=UAblue,bg=UAgrey}
\setbeamercolor*{palette quaternary}{fg=UAblue,bg=UAgrey}

\setbeamercolor*{sidebar}{fg=UAblue,bg=UAgrey}

\setbeamercolor*{palette sidebar primary}{fg=UAgrey}
\setbeamercolor*{palette sidebar secondary}{fg=UAgrey}
\setbeamercolor*{palette sidebar tertiary}{fg=UAgrey}
\setbeamercolor*{palette sidebar quaternary}{fg=UAgrey}

\setbeamercolor{title}{fg=UAblue,bg=UAgrey}
\setbeamercolor{titlelike}{fg=UAblue,bg=UAgrey}
\setbeamercolor{frametitle}{bg=UAgrey,fg=UAblue}
\setbeamercolor{frametitle right}{fg=UAgrey}


\setbeamercolor*{separation line}{fg=UAblue}
\setbeamercolor*{fine separation line}{fg=UAblue}

\mode
<all>
